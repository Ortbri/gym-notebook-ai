{
  "expo": {
    "name": "Gym Notebook",
    "slug": "gym-notebook",
    "version": "1.0.0",
    "newArchEnabled": true,
    "scheme": "gym-notebook",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
         "expo-splash-screen",
        {
          "backgroundColor": "#fff",
          "image": "./assets/splash.png",
          "dark": {
            "image": "./assets/splash.png",
            "backgroundColor": "#1a1a1a"
          },
          "imageWidth": 300
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          },
          "android": {
            "minSdkVersion": 26
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Satoshi-Black.ttf",
            "./assets/fonts/Satoshi-Bold.ttf",
            "./assets/fonts/Satoshi-Light.ttf",
            "./assets/fonts/Satoshi-Regular.ttf"
          ]
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "gym-notebook",
          "organization": "colony"
        }
      ],
      "expo-secure-store",
      "react-native-edge-to-edge",
      "expo-router",
      "react-native-bottom-tabs",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true,
      "reactServerFunctions": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ortbit02.gymnotebook",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ortbit02.gymnotebook"
    },
    "extra": {
      "eas": {
        "projectId": "719d1bcf-7ef8-4105-a14a-c67a13fe845a"
      },
      "router": {
        "origin": "https://localhost:8081"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/719d1bcf-7ef8-4105-a14a-c67a13fe845a"
    },
    "owner": "colony-apps",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}