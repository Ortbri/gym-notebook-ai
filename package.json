{
  "name": "gym-notebook",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "expo run:ios --device",
    "android": "expo run:android --device",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "clean:proj": "rm -rf .expo && rm -rf node_modules && rm -rf pnpm-lock.yaml && rm -rf ios && rm -rf android",
    "db:gen": "drizzle-kit generate",
    "db:mig": "drizzle-kit migrate"
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^0.9.0",
    "@clerk/clerk-expo": "^2.9.6",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^7.0.3",
    "babel-plugin-inline-import": "^3.0.0",
    "burnt": "^0.13.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.41.0",
    "expo": "^52.0.42",
    "expo-build-properties": "~0.13.2",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.4",
    "expo-dev-launcher": "^5.0.17",
    "expo-drizzle-studio-plugin": "^0.1.2",
    "expo-haptics": "~12.0.0",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.6",
    "expo-secure-store": "^14.0.1",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.77.2",
    "react-native-bottom-tabs": "^0.9.0",
    "react-native-edge-to-edge": "1.4.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-ios-context-menu": "^3.1.1",
    "react-native-ios-utilities": "^5.1.4",
    "react-native-nitro-modules": "0.24.1",
    "react-native-reanimated": "3.17.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-unistyles": "3.0.0-nightly-20250326",
    "react-native-web": "~0.19.10",
    "zeego": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "drizzle-kit": "^0.30.6",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true
}
